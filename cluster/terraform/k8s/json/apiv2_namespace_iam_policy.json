{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::channel-firmware",
                "arn:aws:s3:::main-firmware",
                "arn:aws:s3:::channel-firmware/*",
                "arn:aws:s3:::main-firmware/*"
            ]
        }
    ]
}

